@model User
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Hau Nguyen">
    <meta name="keywords" content="au theme template">

    <!-- Title Page-->
    <title>Register</title>

    <!-- Fontfaces CSS-->
    <link href="~/Admindash/css/font-face.css" rel="stylesheet" media="all">
    <link href="~/Admindash/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="~/Admindash/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="~/Admindash/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

    <!-- Bootstrap CSS-->
    <link href="~/Admindash/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

    <!-- Vendor CSS-->
    <link href="~/Admindash/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="~/Admindash/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="~/Admindash/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="~/Admindash/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="~/Admindash/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="~/Admindash/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/Admindash/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="~/Admindash/css/theme.css" rel="stylesheet" media="all">

</head>

<body class="animsition">
    <div class="page-wrapper">
       @* <div class="page-content--bge5">*@
            <div class="container">
                <div class="login-wrap">
                    <div class="login-content">
                        <div class="login-logo">
                            <a href="#">
                               @* <img src="~/Admindash/images/icon/logo.png" alt="CoolAdmin">*@
                               <img src="~/Image/register-removebg-preview.png" width="200" height="100" />
                            </a>
                        </div>
                        <div class="login-form">
                            <form  method="post" asp-action="Register" enctype="multipart/form-data">
                                <div class="form-group">
                                    <label>First Name</label>
                                    <input class="au-input au-input--full" asp-for="FirstName"  placeholder="FirstName">
                                </div>
                                 <div class="form-group">
                                    <label>Last Name</label>
                                    <input class="au-input au-input--full" asp-for="LastName"  placeholder="LasttName">
                                </div>
                                <div class="form-group">
                                    <label>Email Address</label>
                                    <input class="au-input au-input--full" asp-for="Email"  placeholder="Email">
                                </div>
                                 <div class="form-group">
                                    <label>Phone Number</label>
                                    <input class="au-input au-input--full" asp-for="Phone"  placeholder="Phone">
                                </div>
                                 <div class="form-group">
                                    <label>Address</label>
                                    <input class="au-input au-input--full" asp-for="Address"  placeholder="Address">
                                </div>
                                 <div class="form-group">
                                    <label>Uploud Image</label>
                                    <input class="au-input au-input--full" asp-for="ImageFile"  >
                                </div>
                                 <div class="form-group">
                                    <label>UserName</label>
                                    <input class="au-input au-input--full" type="text" name="userName" >
                                </div>
                                 <div class="form-group">
                                    <label>Password</label>
                                    <input class="au-input au-input--full" type="password" name="password" >
                                </div>
                              
                               
                                <button class="au-btn au-btn--block au-btn--green m-b-20" type="submit">register</button>
                               
                            </form>
                            <div class="register-link">
                                <p>
                                    Already have account?
                                    <a asp-controller="auth" asp-action="Login">Sign In</a>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    @*</div>*@

    <!-- Jquery JS-->
    <script src="~/Admindash/vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="~/Admindash/vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="~/Admindash/vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="~/Admindash/vendor/slick/slick.min.js">
    </script>
    <script src="~/Admindash/vendor/wow/wow.min.js"></script>
    <script src="~/Admindash/vendor/animsition/animsition.min.js"></script>
    <script src="~/Admindash/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js">
    </script>
    <script src="~/Admindash/vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="~/Admindash/vendor/counter-up/jquery.counterup.min.js">
    </script>
    <script src="~/Admindash/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="~/Admindash/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="~/Admindash/vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="~/Admindash/vendor/select2/select2.min.js">
    </script>

    <!-- Main JS-->
    <script src="~/Admindash/js/main.js"></script>

</body>

</html>
<!-- end document-->
